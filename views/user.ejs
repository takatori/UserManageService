<!DOCTYPE html>
<html>
<head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css">
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <!-- Latest compiled and minified JavaScript -->
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>
    <script src="/javascripts/users.js"></script>
</head>

<hader>
    <nav>
        <ul class="nav nav-tabs">
            <li class="active"><a href="/">Home</a></li>
            <li><a href="/create">Create </a></li>
        </ul>
    </nav>
</hader>


<body>
    <div class="container-fluid">
    <h1><%= title %></h1>
        <div class="row">
            <div role="main" class="col-md-9">
                <ul class="list-group">
                   <li class="list-group-item"><%= user_data._id %></li>
                   <li class="list-group-item"><%= user_data.stu_id %></li>
                   <li class="list-group-item"><%= user_data.last_name %></li>
                   <li class="list-group-item"><%= user_data.first_name %></li>
                   <li class="list-group-item"><%= user_data.screen_name %></li>
                   <li class="list-group-item"><%= user_data.sex %></li>
                   <li class="list-group-item"><%= user_data.birthday %></li>
                   <li class="list-group-item"><%= user_data.group %></li>
                   <li class="list-group-item"><%= user_data.tel %></li>
                   <li class="list-group-item"><%= user_data.home_tel %></li>
                   <li class="list-group-item"><%= user_data.mail %></li>
                   <li class="list-group-item"><%= user_data.mob_mail %></li>
                   <li class="list-group-item"><%= user_data.addr %></li>
                   <li class="list-group-item"><%= user_data.home_addr %></li>
                </ul>
    <p>
        <a href="" class="btn btn-primary">Update</a>
        <form method="get" action="/delete/<%= user_data._id %>">
            <input type="submit" class="btn btn-danger" value="delete">
        </form>
    </p>

            </div>
            <div role="sidebar" class="col-md-3">
                <aside>
                    <nav>
                        <div class="list-group">
                            <% for(var key in user_list) {%>
                                <% var user = user_list[key]; %>
                                    <a href="/user/<%= user._id %>" class="list-group-item"><%= user.last_name %></a>
                            <% } %>
                        </div>
                    </nav>
                </aside>
            </div>
        </div>
    </div>
</body>
</html>